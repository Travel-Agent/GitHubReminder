{
  "name": "GitHubReminder",
  "description": "Source code for https://githubreminder.org/",
  "version": "0.0.1",
  "author": "Phil Booth <pmbooth@gmail.com",
  "contributors": [
    { "name": "Phil Booth", "email": "pmbooth@gmail.com" }
  ],
  "main": "./server",
  "scripts": {
      "compile-coffee": "./node_modules/.bin/coffee -c --output server server/index.coffee",
      "compile-stylus": "./node_modules/.bin/stylus -o public client/style/GitHubReminder.styl",
      "prefix-css": "./node_modules/.bin/autoprefixer -b 'last 5 versions, > 0.5%, ie 7' public/GitHubReminder.css",
      "compress-css": "./node_modules/.bin/cleancss -e -o public/GitHubReminder.css public/GitHubReminder.css",
      "build": "npm run compile-coffee && npm run compile-stylus && npm run prefix-css && npm run compress-css",
      "start": "node server"
  },
  "dependencies": {
    "coffee-script": "1.6.x",
    "hapi": "1.9.x",
    "handlebars": "1.0.x",
    "mongodb": "1.3.x",
    "pub-sub": "0.1.x",
    "check-types": "0.6.x",
    "nodemailer": "0.5.x",
    "underscore": "1.5.x",
    "uuid": "1.4.x",
    "stylus": "0.37.x",
    "autoprefixer": "0.8.x",
    "clean-css": "1.0.x"
  },
  "devDependencies": {
  },
  "engines": {
    "node": "0.10.x"
  }
}
