{
  "name": "GitHubReminder",
  "private": true,
  "version": "0.0.1",
  "author": "Phil Booth <pmbooth@gmail.com",
  "contributors": [
    { "name": "Phil Booth", "email": "pmbooth@gmail.com" }
  ],
  "main": "./server",
  "scripts": {
      "compile-coffee": "./node_modules/.bin/coffee -c --output server server/index.coffee",
      "concat-styl": "cat client/styl/constants.styl client/styl/elements.styl client/styl/classes.styl > client/styl/GitHubReminder.styl",
      "compile-styl": "./node_modules/.bin/styl -w < client/styl/GitHubReminder.styl > public/GitHubReminder.css",
      "prefix-css": "./node_modules/.bin/autoprefixer -b 'last 3 versions, > 0.5%, ie 7' public/GitHubReminder.css",
      "build": "npm run compile-coffee && npm run compile styl && npm run prefix-css",
      "start": "node server"
  },
  "dependencies": {
    "coffee-script": "1.6.x",
    "hapi": "1.9.x",
    "handlebars": "1.0.x",
    "mongodb": "1.3.x",
    "pub-sub": "0.1.x",
    "check-types": "0.6.x",
    "nodemailer": "0.5.x",
    "underscore": "1.5.x",
    "uuid": "1.4.x",
    "styl": "0.2.x",
    "autoprefixer": "0.7"
  },
  "devDependencies": {
  },
  "engines": {
    "node": "0.10.x"
  }
}
